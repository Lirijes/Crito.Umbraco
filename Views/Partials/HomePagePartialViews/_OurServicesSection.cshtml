@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.HomePage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;


    <section class="our-services">
        <div class="container">
            <h5 class="small-heading">@Model.Value("ourServicesSectionTitle")</h5>
            <h1>@Model.Value("ourServicesSectionHeading")</h1>
            <div class="services-cards">
                @Html.GetBlockListHtml(Model.OurServices)
            </div>
            <a class="transparent-btn service-btn btn-theme" href="@Model.Value("ourServicesSectionButtonUrl")">@Model.Value("ourServicesSectionButtonText")<i class="fa-solid fa-arrow-up-right"></i></a>
        </div>
    </section>